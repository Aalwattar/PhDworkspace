/* Contains structure needed for storing global command line variables */
struct globalArgs {
    char *dfg_filename;      // -f option
    char *dotgraph_filename; // -g option
    int no_of_nodes;         // -n option
    int dep_per_node;        // -d option
    int no_of_types;         // -t option
    int total_dep_no;        // -a option
}; 


